---
import Container from "@components/container.astro";
import ContainerXL from "@components/container-xl.astro";
import Hero from "@components/hero.astro";
import HeroSecondary from "@components/hero-secondary.astro";
import Cta from "@components/cta.astro";
import Grid from "@components/grid-2x2.astro";
import Cards from "@components/cards.astro";
import BlogSlider from "@components/slider-content.astro";
import GallerySlider from "@components/slider-services.astro";
import ContactSimple from "@components/contactform-simple.astro";
import Logos from "@components/logos.astro";
import Layout from "@layouts/Layout.astro";
---

<Layout title="">
  <ContainerXL>
    <Hero />
    <HeroSecondary />
  </ContainerXL>
  <Container>
    <Grid />
    <Logos />
    <BlogSlider />    
    <GallerySlider />
    <Cta />
  </Container>
  <ContainerXL>
    <Cards />
    <ContactSimple />
  </ContainerXL>
</Layout>
